<%
const ghanorPage = _.cms('ghanor')
const { slug, content } = await _.resolveRoute()
_.breadcrumbs = [{
  name: ghanorPage.title,
  link: `/ghanor`
}, {
  name: content.title,
  link: `/ghanor/${slug}`
}]
if (!content.sections || !content.sections.length) {
  content.sections = [
    { type: 'breadcrumbs' },
    { type: 'page-title' },
    { type: 'ghanor-pages' }
  ]
} else if (!content.sections.find(({ type }) => type === 'ghanor-pages')) {
  content.sections.push({ type: 'ghanor-pages' })
}
%>

<%- await include('@/view') %>


